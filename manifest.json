      "label": "Contestants showcase page with candidate profiles"
    },
    {
      "src": "/assets/images/screenshots/gallery-page.png",
      "sizes": "1280x720",
      "type": "image/png",
      "form_factor": "wide",
      "label": "Photo gallery with pageant highlights and events"
    }
  ],
  "shortcuts": [
    {
      "name": "Contestants",
      "short_name": "Contestants",
      "description": "View all contestants and their profiles",
      "url": "/contestants",
      "icons": [
        {
          "src": "/assets/images/icons/contestants-shortcut.png",
          "sizes": "96x96",
          "type": "image/png"
        }
      ]
    },
    {
      "name": "Events",
      "short_name": "Events",
      "description": "Check event schedule and details",
      "url": "/events",
      "icons": [
        {
          "src": "/assets/images/icons/events-shortcut.png",
          "sizes": "96x96",
          "type": "image/png"
        }
      ]
    },
    {
      "name": "Gallery",
      "short_name": "Gallery",
      "description": "Browse photo gallery and highlights",
      "url": "/gallery",
      "icons": [
        {
          "src": "/assets/images/icons/gallery-shortcut.png",
          "sizes": "96x96",
          "type": "image/png"
        }
      ]
    },
    {
      "name": "Vote",
      "short_name": "Vote",
      "description": "Cast your vote for your favorite contestant",
      "url": "/voting",
      "icons": [
        {
          "src": "/assets/images/icons/vote-shortcut.png",
          "sizes": "96x96",
          "type": "image/png"
        }
      ]
    }
  ],
  "prefer_related_applications": false,
  "edge_side_panel": {
    "preferred_width": 400
  },
  "launch_handler": {
    "client_mode": "navigate-existing"
  },
  "protocol_handlers": [
    {
      "protocol": "web+pageant",
      "url": "/share?url=%s"
    }
  ],
  "share_target": {
    "action": "/share",
    "method": "GET",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url"
    }
  },
  "file_handlers": [
    {
      "action": "/upload",
      "accept": {
        "image/*": [".jpg", ".jpeg", ".png", ".webp", ".gif"]
      }
    }
  ],
  "display_override": [
    "window-controls-overlay",
    "minimal-ui",
    "browser"
  ]
}
