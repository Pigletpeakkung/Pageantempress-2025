{
  "name": "pageantempress-2025",
  "version": "1.0.0",
  "description": "PageantEmpress 2025 - Beauty Pageant Competition Website",
  "scripts": {
    "dev": "concurrently \"sass --watch src/styles:css\" \"http-server . -p 3000 -c-1\"",
    "build:css": "sass src/styles/main.scss css/main.css --style compressed",
    "build": "npm run build:css",
    "start": "http-server . -p 3000",
    "deploy": "npm run build && gh-pages -d ."
  },
  "devDependencies": {
    "sass": "^1.69.5",
    "http-server": "^14.1.1",
    "concurrently": "^8.2.0",
    "gh-pages": "^6.1.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Pigletpeakkung/Pageantempress-2025.git"
  },
  "homepage": "https://pigletpeakkung.github.io/Pageantempress-2025"
}
