{
  "name": "pageantempress",
  "version": "1.0.0",
  "description": "Advanced Web Animation Showcase - Experience the pinnacle of web animation artistry with advanced CSS effects, interactive elements, and cutting-edge performance optimization.",
  "main": "index.html",
  "scripts": {
    "start": "npx serve .",
    "build": "npm run minify:css && npm run minify:js && npm run optimize:images",
    "dev": "npx live-server --port=3000 --host=localhost",
    "test": "npm run lighthouse && npm run accessibility",
    "lighthouse": "npx lighthouse http://localhost:3000 --output=html --output-path=./reports/lighthouse-report.html",
    "accessibility": "npx pa11y http://localhost:3000",
    "minify:css": "npx cleancss -o dist/css/main.min.css css/main.css css/instagram.css",
    "minify:js": "npx terser js/main.js js/performance-monitor.js js/particle-system.js js/neural-network.js js/quantum-effects.js js/accessibility.js -o dist/js/main.min.js",
    "optimize:images": "npx imagemin assets/images/* --out-dir=dist/assets/images",
    "serve": "npx http-server -p 8080 -c-1",
    "validate:html": "npx html-validate index.html",
    "format": "npx prettier --write .",
    "lint:css": "npx stylelint 'css/*.css'",
    "lint:js": "npx eslint js/*.js",
    "performance": "npm run lighthouse && npm run bundle-analyzer",
    "bundle-analyzer": "npx webpack-bundle-analyzer dist/js/main.min.js",
    "security": "npx retire",
    "audit": "npm audit",
    "precommit": "npm run lint:css && npm run lint:js && npm run validate:html",
    "deploy": "npm run build && npx netlify deploy --prod"
  },
  "keywords": [
    "web-animation",
    "css-effects",
    "interactive-design",
    "performance-optimization",
    "pageant",
    "modern-web-development",
    "ui-ux",
    "accessibility",
    "progressive-web-app",
    "particle-effects",
    "neural-network",
    "quantum-effects"
  ],
  "author": {
    "name": "George G. & Thanattsitt S.",
    "email": "info@pageantempress.com",
    "url": "https://pageantempress.com"
  },
  "contributors": [
    {
      "name": "George G.",
      "email": "george@pageantempress.com",
      "role": "Lead Developer & Animation Architect"
    },
    {
      "name": "Thanattsitt S.",
      "email": "thanattsitt@pageantempress.com",
      "role": "UX Designer & Visual Artist"
    }
  ],
  "license": "MIT",
  "homepage": "https://pageantempress.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/pageantempress/pageantempress.git"
  },
  "bugs": {
    "url": "https://github.com/pageantempress/pageantempress/issues"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ],
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "autoprefixer": "^10.4.16",
    "clean-css-cli": "^5.6.2",
    "eslint": "^8.52.0",
    "html-validate": "^8.7.0",
    "http-server": "^14.1.1",
    "imagemin": "^8.0.1",
    "imagemin-mozjpeg": "^10.0.0",
    "imagemin-pngquant": "^9.0.2",
    "lighthouse": "^11.2.0",
    "live-server": "^1.2.2",
    "netlify-cli": "^17.0.0",
    "pa11y": "^7.0.0",
    "postcss": "^8.4.31",
    "postcss-cli": "^10.1.0",
    "prettier": "^3.0.3",
    "retire": "^4.0.0",
    "serve": "^14.2.1",
    "stylelint": "^15.11.0",
    "stylelint-config-standard": "^34.0.0",
    "terser": "^5.24.0",
    "webpack-bundle-analyzer": "^4.9.1"
  },
  "dependencies": {
    "aos": "^2.3.4",
    "gsap": "^3.12.2",
    "swiper": "^10.3.1"
  },
  "peerDependencies": {
    "font-awesome": "^6.4.0"
  },
  "funding": {
    "type": "individual",
    "url": "https://github.com/sponsors/pageantempress"
  },
  "config": {
    "port": 3000,
    "host": "localhost"
  },
  "private": false
}
